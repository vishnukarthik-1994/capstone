<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sign_up_navigation"
    app:startDestination="@id/nav_register_username">
    <fragment
        android:id="@+id/nav_register_username"
        android:name="com.example.dfu_app.ui.register.RegisterNameFragment"
        tools:layout="@layout/fragment_register_username" >
        <action
            android:id="@+id/action_nav_register_username_to_nav_register_userinfo"
            app:destination="@id/nav_register_userinfo" />
    </fragment>

    <fragment
        android:id="@+id/nav_register_userinfo"
        android:name="com.example.dfu_app.ui.register.RegisterUserInfoFragment"
        tools:layout="@layout/fragment_register_userinfo" />

</navigation>